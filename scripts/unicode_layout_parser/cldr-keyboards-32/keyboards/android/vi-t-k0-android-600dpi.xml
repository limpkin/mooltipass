<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="vi-t-k0-android-600dpi">
	<version platform="4.4" number="$Revision: 11914 $"/>
	<names>
		<name value="Vietnamese (600dpi)"/>
	</names>
	<keyMap>
		<map iso="D01" to="q"/>
		<map iso="D02" to="w"/>
		<map iso="D03" to="e" longPress="è é ẻ ẽ ẹ ê ề ế ể ễ ệ"/>

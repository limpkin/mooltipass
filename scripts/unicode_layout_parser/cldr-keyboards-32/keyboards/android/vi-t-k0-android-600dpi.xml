		<map iso="D09" to="o" longPress="ò ó ỏ õ ọ ô ồ ố ổ ỗ ộ ơ ờ ớ ở ỡ ợ"/>
		<map iso="D10" to="p"/>
		<map iso="C01" to="a" longPress="à á ả ã ạ ă ằ ắ ẳ ẵ ặ â ầ ấ ẩ ẫ ậ"/>
		<map iso="C02" to="s"/>
		<map iso="C03" to="d" longPress="đ"/>
		<map iso="C04" to="f"/>
		<map iso="C05" to="g"/>
		<map iso="C06" to="h"/>
		<map iso="C07" to="j"/>
		<map iso="C08" to="k"/>
		<map iso="C09" to="l"/>
		<map iso="B01" to="z"/>
		<map iso="B02" to="x"/>
		<map iso="B03" to="c"/>
		<map iso="B04" to="v"/>
		<map iso="B05" to="b"/>
		<map iso="B06" to="n"/>
		<map iso="B07" to="m"/>
		<map iso="B08" to="!"/> <!-- , -->
		<map iso="B09" to="?"/> <!-- . -->
		<map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
		<map iso="A02" to=" "/> <!-- (key to left of space) -->
		<map iso="A03" to=","/> <!-- space -->
		<map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
	</keyMap>
	<keyMap modifiers="shift caps">
		<map iso="D01" to="Q"/>
		<map iso="D02" to="W"/>
		<map iso="D03" to="E" longPress="È É Ẻ Ẽ Ẹ Ê Ề Ế Ể Ễ Ệ"/>
		<map iso="D04" to="R"/>
		<map iso="D05" to="T"/>
		<map iso="D06" to="Y" longPress="Ỳ Ý Ỷ Ỹ Ỵ"/>
		<map iso="D07" to="U" longPress="Ù Ú Ủ Ũ Ụ Ư Ừ Ứ Ử Ữ Ự"/>
		<map iso="D08" to="I" longPress="Ì Í Ỉ Ĩ Ị"/>
		<map iso="D09" to="O" longPress="Ò Ó Ỏ Õ Ọ Ô Ồ Ố Ổ Ỗ Ộ Ơ Ờ Ớ Ở Ỡ Ợ"/>
		<map iso="D10" to="P"/>
		<map iso="C01" to="A" longPress="À Á Ả Ã Ạ Ă Ằ Ắ Ẳ Ẵ Ặ Â Ầ Ấ Ẩ Ẫ Ậ"/>
		<map iso="C02" to="S"/>
		<map iso="C03" to="D" longPress="Đ"/>
		<map iso="C04" to="F"/>
		<map iso="C05" to="G"/>
		<map iso="C06" to="H"/>
		<map iso="C07" to="J"/>
		<map iso="C08" to="K"/>
		<map iso="C09" to="L"/>

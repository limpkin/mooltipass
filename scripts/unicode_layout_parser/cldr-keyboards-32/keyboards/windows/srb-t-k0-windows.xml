		<map iso="E09" to="ჹ"/> <!-- 9 -->
		<map iso="E10" to="ჰ"/> <!-- 0 -->
		<map iso="E11" to="-"/>
		<map iso="E12" to="="/>
		<map iso="D02" to="ყ"/> <!-- W -->
		<map iso="D03" to="უ"/> <!-- E -->
		<map iso="D04" to="ო"/> <!-- R -->
		<map iso="D05" to="ბ"/> <!-- T -->
		<map iso="D06" to="ნ"/> <!-- Y -->
		<map iso="D07" to="ქ"/> <!-- U -->
		<map iso="D08" to="ფ"/> <!-- I -->
		<map iso="D09" to="ღ"/> <!-- O -->
		<map iso="D10" to="მ"/> <!-- P -->
		<map iso="D11" to="["/>
		<map iso="D12" to="]"/>
		<map iso="C01" to="ტ"/> <!-- A -->
		<map iso="C02" to="ა"/> <!-- S -->
		<map iso="C03" to="ე"/> <!-- D -->
		<map iso="C04" to="თ"/> <!-- F -->
		<map iso="C05" to="ვ"/> <!-- G -->
		<map iso="C06" to="პ"/> <!-- H -->
		<map iso="C07" to="რ"/> <!-- J -->
		<map iso="C08" to="ჟ"/> <!-- K -->
		<map iso="C09" to="ი"/> <!-- L -->
		<map iso="C10" to=";"/>
		<map iso="C11" to="&apos;"/>
		<map iso="C12" to="\"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="შ"/> <!-- Z -->
		<map iso="B02" to="ს"/> <!-- X -->
		<map iso="B03" to="დ"/> <!-- C -->

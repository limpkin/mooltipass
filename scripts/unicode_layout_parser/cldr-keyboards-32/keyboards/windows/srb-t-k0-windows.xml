<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="srb-t-k0-windows">
	<version platform="10" number="$Revision: 13680 $"/>
	<names>
		<name value="Sora"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="`"/>
		<map iso="E01" to="ჱ"/> <!-- 1 -->
		<map iso="E02" to="ჲ"/> <!-- 2 -->
		<map iso="E03" to="ჳ"/> <!-- 3 -->
		<map iso="E04" to="ჴ"/> <!-- 4 -->

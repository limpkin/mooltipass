<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE keyboard SYSTEM "../dtd/ldmlKeyboard.dtd">
<keyboard locale="ar-t-k0-windows-azerty">
	<version platform="10" number="$Revision: 13265 $"/>
	<names>
		<name value="Arabic (102) AZERTY"/>
	</names>
	<settings fallback="omit"/>
	<keyMap>
		<map iso="E00" to="&gt;"/> <!-- ` -->
		<map iso="E01" to="&amp;"/> <!-- 1 -->
		<map iso="E02" to="é"/> <!-- 2 -->
		<map iso="E03" to="\u{22}"/> <!-- 3  to= "  -->
		<map iso="E04" to="&apos;"/> <!-- 4 -->
		<map iso="E05" to="("/> <!-- 5 -->
		<map iso="E06" to="-"/> <!-- 6 -->
		<map iso="E07" to="è"/> <!-- 7 -->
		<map iso="E08" to="_"/> <!-- 8 -->
		<map iso="E09" to="ç"/> <!-- 9 -->
		<map iso="E10" to="à"/> <!-- 0 -->
		<map iso="E11" to=")"/> <!-- - -->
		<map iso="E12" to="="/>
		<map iso="D01" to="ض"/> <!-- Q -->
		<map iso="D02" to="ص"/> <!-- W -->
		<map iso="D03" to="ث"/> <!-- E -->
		<map iso="D04" to="ق"/> <!-- R -->
		<map iso="D05" to="ف"/> <!-- T -->
		<map iso="D06" to="غ"/> <!-- Y -->
		<map iso="D07" to="ع"/> <!-- U -->
		<map iso="D08" to="ه"/> <!-- I -->
		<map iso="D09" to="خ"/> <!-- O -->
		<map iso="D10" to="ح"/> <!-- P -->
		<map iso="D11" to="ج"/> <!-- [ -->
		<map iso="D12" to="د"/> <!-- ] -->
		<map iso="C01" to="ش"/> <!-- A -->
		<map iso="C02" to="س"/> <!-- S -->
		<map iso="C03" to="ي"/> <!-- D -->
		<map iso="C04" to="ب"/> <!-- F -->
		<map iso="C05" to="ل"/> <!-- G -->
		<map iso="C06" to="ا"/> <!-- H -->
		<map iso="C07" to="ت"/> <!-- J -->
		<map iso="C08" to="ن"/> <!-- K -->
		<map iso="C09" to="م"/> <!-- L -->
		<map iso="C10" to="ك"/> <!-- ; -->
		<map iso="C11" to="ط"/> <!-- ' -->
		<map iso="C12" to="ذ"/> <!-- (key to right of ') -->
		<map iso="B00" to="\"/> <!-- (key to left of Z) -->
		<map iso="B01" to="ئ"/> <!-- Z -->
		<map iso="B02" to="ء"/> <!-- X -->
		<map iso="B03" to="ؤ"/> <!-- C -->
		<map iso="B04" to="ر"/> <!-- V -->
		<map iso="B05" to="لا"/> <!-- B -->
		<map iso="B06" to="ى"/> <!-- N -->
		<map iso="B07" to="ة"/> <!-- M -->
		<map iso="B08" to="و"/> <!-- , -->
		<map iso="B09" to="ز"/> <!-- . -->
		<map iso="B10" to="ظ"/> <!-- / -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="shift+caps?">
		<map iso="E00" to="&lt;"/> <!-- `  base=> -->
		<map iso="E01" to="1"/> <!--   base=& -->
		<map iso="E02" to="2"/> <!--   base=é -->
		<map iso="E03" to="3"/> <!--   base=" -->
		<map iso="E04" to="4"/> <!--   base=' -->
		<map iso="E05" to="5"/> <!--   base=( -->
		<map iso="E06" to="6"/> <!--   base=- -->
		<map iso="E07" to="7"/> <!--   base=è -->
		<map iso="E08" to="8"/> <!--   base=_ -->
		<map iso="E09" to="9"/> <!--   base=ç -->
		<map iso="E10" to="0"/> <!--   base=à -->
		<map iso="E11" to="°"/> <!-- -  base=) -->
		<map iso="E12" to="+"/> <!-- = -->
		<map iso="D01" to="\u{64E}"/> <!-- Q  base=ض  to= َ  -->
		<map iso="D02" to="\u{64B}"/> <!-- W  base=ص  to= ً  -->
		<map iso="D03" to="\u{64F}"/> <!-- E  base=ث  to= ُ  -->
		<map iso="D04" to="\u{64C}"/> <!-- R  base=ق  to= ٌ  -->
		<map iso="D05" to="لإ"/> <!-- T  base=ف -->
		<map iso="D06" to="إ"/> <!-- Y  base=غ -->
		<map iso="D07" to="‘"/> <!-- U  base=ع -->
		<map iso="D08" to="÷"/> <!-- I  base=ه -->
		<map iso="D09" to="×"/> <!-- O  base=خ -->
		<map iso="D10" to="؛"/> <!-- P  base=ح -->
		<map iso="D11" to="}"/> <!-- [  base=ج -->
		<map iso="D12" to="{"/> <!-- ]  base=د -->
		<map iso="C01" to="\"/> <!-- A  base=ش -->
		<map iso="C03" to="]"/> <!-- D  base=ي -->
		<map iso="C04" to="["/> <!-- F  base=ب -->
		<map iso="C05" to="لأ"/> <!-- G  base=ل -->
		<map iso="C06" to="أ"/> <!-- H  base=ا -->
		<map iso="C07" to="ـ"/> <!-- J  base=ت -->
		<map iso="C08" to="،"/> <!-- K  base=ن -->
		<map iso="C09" to="/"/> <!-- L  base=م -->
		<map iso="C10" to=":"/> <!-- ;  base=ك -->
		<map iso="C11" to="\u{22}"/> <!-- '  base=ط  to= "  -->
		<map iso="C12" to="\u{651}"/> <!-- (key to right of ')  base=ذ  to= ّ  -->
		<map iso="B00" to="|"/> <!-- (key to left of Z)  base=\ -->
		<map iso="B01" to="~"/> <!-- Z  base=ئ -->
		<map iso="B02" to="\u{652}"/> <!-- X  base=ء  to= ْ  -->
		<map iso="B03" to="\u{650}"/> <!-- C  base=ؤ  to= ِ  -->
		<map iso="B04" to="\u{64D}"/> <!-- V  base=ر  to= ٍ  -->
		<map iso="B05" to="لآ"/> <!-- B  base=لا -->
		<map iso="B06" to="آ"/> <!-- N  base=ى -->
		<map iso="B07" to="’"/> <!-- M  base=ة -->
		<map iso="B08" to=","/> <!--   base=و -->
		<map iso="B09" to="."/> <!--   base=ز -->
		<map iso="B10" to="؟"/> <!-- /  base=ظ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="altR+caps?+shift? ctrl+alt+caps?+shift?">
		<map iso="E04" to="¤"/> <!-- 4  base=' -->
		<map iso="D01" to="\u{651}\u{64E}"/> <!-- Q  base=ض  to= َّ  -->
		<map iso="D02" to="\u{651}\u{64B}"/> <!-- W  base=ص  to= ًّ  -->
		<map iso="D03" to="\u{651}\u{64F}"/> <!-- E  base=ث  to= ُّ  -->
		<map iso="D04" to="\u{651}\u{64C}"/> <!-- R  base=ق  to= ٌّ  -->
		<map iso="B03" to="\u{651}\u{650}"/> <!-- C  base=ؤ  to= ِّ  -->
		<map iso="B04" to="\u{651}\u{64D}"/> <!-- V  base=ر  to= ٍّ  -->
	</keyMap>
	<keyMap modifiers="ctrl+caps?">
		<map iso="D11" to="\u{1B}"/> <!-- [  base=ج -->
		<map iso="D12" to="\u{1D}"/> <!-- ]  base=د -->
		<map iso="C11" to="\u{1C}"/> <!-- '  base=ط -->
		<map iso="C12" to="\u{1C}"/> <!-- (key to right of ')  base=ذ -->
		<map iso="A03" to=" "/> <!-- space -->
	</keyMap>
	<keyMap modifiers="ctrl+shift+caps?">
		<map iso="E01" to="\u{200D}"/> <!-- 1  base=& -->
		<map iso="E02" to="\u{200C}"/> <!-- 2  base=é -->
		<map iso="E03" to="\u{200E}"/> <!-- 3  base=" -->
		<map iso="E04" to="\u{200F}"/> <!-- 4  base=' -->
	</keyMap>
</keyboard>
